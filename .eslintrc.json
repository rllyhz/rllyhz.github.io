{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-console": "warn",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "quotes": ["error", "double"],
        "linebreak-style": "off",
        "max-classes-per-file": "off",
        "no-plusplus":"off",
        "class-methods-use-this": "off",
        "no-param-reassign": "off",
        "import/no-cycle": [2, { "maxDepth": 1 }]
    }
}
